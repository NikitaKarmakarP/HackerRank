#include <iostream>
using namespace std;

void staircase(int n) {
    for (int i = 1; i <= n; i++) {
        // Print leading spaces
        for (int j = 0; j < n - i; j++) {
            cout << " ";
        }
        // Print '#' characters
        for (int k = 0; k < i; k++) {
            cout << "#";
        }
        // Move to the next line
        cout << endl;
    }
}

int main() {
    int n;
    cin >> n;
    staircase(n);
    return 0;
}
